{
  "name": "List Screen Template",
  "description": "Template for displaying lists of items",
  "figmaSpecs": {
    "frame": {
      "width": 375,
      "height": 812
    },
    "layers": [
      {
        "type": "navigation",
        "hasBackButton": true,
        "hasSearch": true
      },
      {
        "type": "list",
        "itemTemplate": "card",
        "spacing": 12
      },
      {
        "type": "fab",
        "position": "bottom-right"
      }
    ]
  },
  "components": [
    "Header",
    "SearchBar",
    "ListItem",
    "FAB"
  ],
  "states": [
    "default",
    "loading",
    "empty",
    "error"
  ]
}